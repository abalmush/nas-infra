market.inexlab.com {
	tls {
		dns cloudflare {env.CLOUDFLARE_API_TOKEN}
	}
	reverse_proxy market-data-frontend:3000
}

context.inexlab.com {
	tls {
		dns cloudflare {env.CLOUDFLARE_API_TOKEN}
	}
	reverse_proxy context-app:3000
}

dsm.inexlab.com {
	tls {
		dns cloudflare {env.CLOUDFLARE_API_TOKEN}
	}
	reverse_proxy https://host.docker.internal:5001 {
		transport http {
			tls_insecure_skip_verify
		}
	}
}
